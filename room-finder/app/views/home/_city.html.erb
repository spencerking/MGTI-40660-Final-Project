<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Age</th>
			<th>Gender</th>
			<th>Company</th>
			<th>Apartment Complex</th>
		</tr>
	</thead>
	<tbody>
		<% @city.Users.each do |user| %> 
			<tr>
				<td><%= user.name %></td>
				<td><%= user.age %></td>
				<td><%= user.gender %></td>
				<td><%= user.Company %></td>
				<td><%= user.Apt_complex %></td>
			</tr>
		<% end %>
	</tbody>
</table>